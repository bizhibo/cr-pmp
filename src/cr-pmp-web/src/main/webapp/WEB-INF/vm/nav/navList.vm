<table class="table table-bordered">
    <thead>
        <tr>
            <th>项目</th>
            <th>进度</th>
            <th>任务</th>
            <th>日期</th>
            <th>操作</th>
        </tr>
    </thead>
    <tbody>
		#foreach($n in $list)
        <tr>
            <td>$!{n.id}</td>
            <td>
				$!{n.name}
            </td>
            <td>$!{n.sex}</td>
            <td>$!{n.age}</td>
            <td>
               $!{n.aa}
            </td>
        </tr>
		#end
    </tbody>
</table>
<div>
	#set($pageModule = $mainModule.getTarget("/nav/pageList.do"))
	#showAjaxPage($pageModule $list)
</div>
